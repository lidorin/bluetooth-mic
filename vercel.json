{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/bundle.js",
      "dest": "/public/bundle.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
} 